Cell[BoxData[
 RowBox[{
  RowBox[{"chrome2asso", "[", "source_", "]"}], ":=", 
  RowBox[{"Block", "[", 
   RowBox[{
    RowBox[{"{", "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"urlNames", "=", 
      RowBox[{"Flatten", "/@", 
       RowBox[{"StringCases", "[", 
        RowBox[{
         RowBox[{"StringCases", "[", 
          RowBox[{"source", ",", 
           RowBox[{"Shortest", "[", 
            RowBox[{
            "\"\<<DT><A HREF=\\\"http\>\"", "~~", "___", "~~", 
             "\"\<</A>\>\""}], "]"}]}], "]"}], ",", 
         RowBox[{
          RowBox[{
           RowBox[{"Shortest", "[", 
            RowBox[{
            "\"\<<A HREF=\\\"\>\"", "~~", "url__", "~~", "\"\<\\\"\>\""}], 
            "]"}], "~~", "__", "~~", "\"\<>\>\"", "~~", "name__", "~~", 
           "\"\<</A>\>\""}], "\[RuleDelayed]", 
          RowBox[{"{", 
           RowBox[{"url", ",", "name"}], "}"}]}]}], "]"}]}]}], ";", "\n", 
     RowBox[{"assoUrlNames", "=", 
      RowBox[{"GroupBy", "[", 
       RowBox[{"urlNames", ",", 
        RowBox[{"First", "\[Rule]", "Last"}], ",", 
        RowBox[{
         RowBox[{"First", "@", 
          RowBox[{"SortBy", "[", 
           RowBox[{"#", ",", "StringLength"}], "]"}]}], "&"}]}], "]"}]}], ";",
      "\[IndentingNewLine]", 
     RowBox[{"dlList", "=", 
      RowBox[{"StringCases", "[", 
       RowBox[{"source", ",", 
        RowBox[{"{", 
         RowBox[{
          RowBox[{"Shortest", "[", 
           RowBox[{
           "\"\<<DT><A HREF=\\\"http\>\"", "~~", "___", "~~", 
            "\"\<</A>\>\""}], "]"}], ",", 
          RowBox[{"Shortest", "[", 
           RowBox[{"\"\<<DT><H3\>\"", "~~", "___", "~~", "\"\<</H3>\>\""}], 
           "]"}]}], "}"}]}], "]"}]}], ";", "\n", 
     RowBox[{"resListFlatten", "=", 
      RowBox[{"getURL", "/@", 
       RowBox[{"Rest", "[", 
        RowBox[{"removeTagSkip", "/@", "dlList"}], "]"}]}]}], ";", "\n", 
     RowBox[{"resList", "=", 
      RowBox[{"SplitBy", "[", 
       RowBox[{"resListFlatten", ",", 
        RowBox[{
         RowBox[{"StringContainsQ", "[", 
          RowBox[{"#", ",", "\"\<http\>\""}], "]"}], "&"}]}], "]"}]}], ";", 
     "\n", 
     RowBox[{"posList", "=", 
      RowBox[{"Position", "[", 
       RowBox[{"resList", ",", 
        RowBox[{"_String", "?", 
         RowBox[{"(", 
          RowBox[{
           RowBox[{"StringContainsQ", "[", 
            RowBox[{
             RowBox[{"ToString", "@", "#"}], ",", "\"\<http\>\""}], "]"}], 
           "&"}], ")"}]}]}], "]"}]}], ";", "\n", 
     RowBox[{"checker", "=", 
      RowBox[{
       RowBox[{"(", 
        RowBox[{
         RowBox[{"posList", "[", 
          RowBox[{"[", 
           RowBox[{"All", ",", "1"}], "]"}], "]"}], "//", "Union"}], ")"}], 
       "~", "Intersection", "~", 
       RowBox[{"(", 
        RowBox[{
         RowBox[{"(", 
          RowBox[{
           RowBox[{"posList", "[", 
            RowBox[{"[", 
             RowBox[{"All", ",", "1"}], "]"}], "]"}], "//", "Union"}], ")"}], 
         "-", "1"}], ")"}]}]}], ";", "\n", 
     RowBox[{"posListName", "=", 
      RowBox[{"Position", "[", 
       RowBox[{"resList", ",", 
        RowBox[{"_String", "?", 
         RowBox[{"(", 
          RowBox[{
           RowBox[{"StringFreeQ", "[", 
            RowBox[{
             RowBox[{"ToString", "@", "#"}], ",", "\"\<http\>\""}], "]"}], 
           "&"}], ")"}]}]}], "]"}]}], ";", "\n", "\[IndentingNewLine]", 
     RowBox[{"If", "[", 
      RowBox[{
       RowBox[{
        RowBox[{"And", "@@", 
         RowBox[{"OddQ", "/@", 
          RowBox[{"Union", "[", 
           RowBox[{"posListName", "[", 
            RowBox[{"[", 
             RowBox[{"All", ",", "1"}], "]"}], "]"}], "]"}]}]}], "||", 
        RowBox[{"checker", "\[NotEqual]", 
         RowBox[{"{", "}"}]}]}], ",", "\n", 
       RowBox[{
        RowBox[{"leafPos", "=", 
         RowBox[{"KeyValueMap", "[", 
          RowBox[{"List", ",", 
           RowBox[{"GroupBy", "[", 
            RowBox[{"posListName", ",", 
             RowBox[{"First", "\[Rule]", "Last"}], ",", "Max"}], "]"}]}], 
          "]"}]}], ";", "\n", 
        RowBox[{"assoLeaf", "=", 
         RowBox[{"Thread", "[", 
          RowBox[{
           RowBox[{"Extract", "[", 
            RowBox[{"resList", ",", "leafPos"}], "]"}], "->", 
           RowBox[{"Extract", "[", 
            RowBox[{"resList", ",", 
             RowBox[{
              RowBox[{"List", "/@", 
               RowBox[{"leafPos", "[", 
                RowBox[{"[", 
                 RowBox[{"All", ",", "1"}], "]"}], "]"}]}], "+", "1"}]}], 
            "]"}]}], "]"}]}], ";", "\n", 
        RowBox[{"asso", "=", 
         RowBox[{"GroupBy", "[", 
          RowBox[{"assoLeaf", ",", 
           RowBox[{"First", "\[Rule]", "Last"}], ",", 
           RowBox[{
            RowBox[{"Union", "@", 
             RowBox[{"Flatten", "@", "#"}]}], "&"}]}], "]"}]}], ";"}], 
       "\[IndentingNewLine]", ",", 
       RowBox[{"Print", "[", "\"\<Something Wrong!\>\"", "]"}]}], "]"}]}]}], 
   "\[IndentingNewLine]", "\[IndentingNewLine]", "]"}]}]], "Input",
 CellChangeTimes->{{3.709253906038377*^9, 3.709253959268481*^9}, {
